<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>SignUpController</servlet-name>
        <servlet-class>Controller.SignUpController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RoleRequestController</servlet-name>
        <servlet-class>Controller.RoleRequestController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SignInController</servlet-name>
        <servlet-class>Controller.SignInController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EventController</servlet-name>
        <servlet-class>Controller.EventController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AwardController</servlet-name>
        <servlet-class>Controller.AwardController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>MeritRequestController</servlet-name>
        <servlet-class>Controller.MeritRequestController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CGPARequestController</servlet-name>
        <servlet-class>Controller.CGPARequestController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ParticipationController</servlet-name>
        <servlet-class>Controller.ParticipationController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RatingController</servlet-name>
        <servlet-class>Controller.RatingController</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>SignUpController</servlet-name>
        <url-pattern>/SignUpController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RoleRequestController</servlet-name>
        <url-pattern>/RoleRequestController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SignInController</servlet-name>
        <url-pattern>/SignInController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EventController</servlet-name>
        <url-pattern>/EventController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AwardController</servlet-name>
        <url-pattern>/AwardController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>MeritRequestController</servlet-name>
        <url-pattern>/MeritRequestController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CGPARequestController</servlet-name>
        <url-pattern>/CGPARequestController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ParticipationController</servlet-name>
        <url-pattern>/ParticipationController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RatingController</servlet-name>
        <url-pattern>/RatingController</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
